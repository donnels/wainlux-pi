;; K6 Version Request (0xFF)
;; 4-byte command to query firmware version

(defattrs :bg-cmd {:fill "#e8f4f8"})

(draw-column-headers)

(draw-box "0xFF (Version Query)" [:box-first {:span 8} :bg-cmd])
(draw-box "0x00 (Reserved)" [:box-related {:span 8} :bg-cmd])
(draw-box "0x04 (Length)" [:box-related {:span 8} :bg-cmd])
(draw-box "0x00 (Reserved)" [:box-last {:span 8} :bg-cmd])
